{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": false,
    "max-line-length": {
      "options": [
        210
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "no-empty-interface": false,
    "no-namespace": false,
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "object-literal-key-quotes": false,
    "only-arrow-functions": false,
    "semicolon": [
      true,
      "never"
    ],
    "space-within-parens": [
      true,
      1
    ],
    "trailing-comma": false,
    "variable-name": [
      true,
      "check-format",
      "allow-snake-case"
    ],
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-type-operator",
      "check-preblock"
    ]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        160
      ]
    }
  }
}
